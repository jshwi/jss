{
  "name": "jss",
  "version": "1.16.0",
  "engines": {
    "node": "16.x"
  },
  "description": "A Flask web application",
  "private": "true",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "audit": "npm-run-all audit:* && npm run toml:sort",
    "audit:js": "npx prettier -c . && npm run lint:js && npm run test:js",
    "audit:py": "poetry run pyaud audit --deploy --rcfile .github/pyaud.toml",
    "build": "npm run webpack:prod",
    "build:dev": "npm run webpack:dev",
    "bump:major": "poetry run bump2version major",
    "bump:minor": "poetry run bump2version minor",
    "bump:patch": "poetry run bump2version patch",
    "clean": "poetry run pyaud clean",
    "create:admin": "poetry run flask create admin",
    "db:upgrade": "poetry run flask db upgrade",
    "format": "npm-run-all format:*",
    "format:misc": "prettier -w .",
    "format:py": "npm-run-all format:py:*",
    "format:py:code": "poetry run pyaud format --fix",
    "format:py:docs": "poetry run pyaud format-docs --fix",
    "format:py:str": "poetry run pyaud format-str --fix",
    "lighthouse": "lighthouse http://localhost:5000 --view --chrome-flags=--headless --output-path=\"$(mktemp).html\"",
    "lint": "npm-run-all lint:*",
    "lint:js": "npx eslint",
    "lint:py": "poetry run pyaud lint",
    "prebuild:dev": "npm run static:clean",
    "prestart:dev:watch": "npm run static:clean",
    "release": "run-s build db:upgrade create:admin",
    "start": "poetry run gunicorn wsgi:app",
    "start:dev": "poetry run flask run",
    "start:dev:watch": "concurrently -n 'WATCH,SERVER' -c 'bgBlue.bold,bgMagenta.bold' 'npm run webpack:watch' 'npm run start:dev'",
    "static:clean": "poetry run flask digest clean",
    "static:compile": "poetry run flask digest compile",
    "test": "npm-run-all test:*",
    "test:js": "jest",
    "test:py": "poetry run pyaud tests",
    "toml:sort": "poetry run toml-sort pyproject.toml --in-place --all",
    "types": "npm-run-all types:*",
    "types:py": "poetry run pyaud typecheck",
    "webpack:dev": "NODE_ENV=development webpack --progress",
    "webpack:prod": "NODE_ENV=production webpack",
    "webpack:watch": "NODE_ENV=development webpack --watch --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jshwi/jss.git"
  },
  "keywords": [
    "Flask",
    "web",
    "application"
  ],
  "author": "Stephen Whitlock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jshwi/jss/issues"
  },
  "homepage": "https://github.com/jshwi/jss#readme",
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier",
      "plugin:json/recommended"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false
    },
    "rules": {
      "no-param-reassign": "off",
      "no-console": "off",
      "no-underscore-dangle": "off",
      "max-len": [
        "error",
        {
          "code": 79,
          "comments": 72,
          "ignoreComments": false,
          "ignoreTrailingComments": false,
          "ignoreUrls": false,
          "ignoreStrings": false,
          "ignoreTemplateLiterals": false,
          "ignoreRegExpLiterals": false
        }
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "__dirname": true,
      "jQuery": true,
      "$": true
    }
  },
  "eslintIgnore": [
    ".mypy_cache/",
    ".pytest_cache/",
    "Pipfile.lock",
    "app/static",
    "dist/",
    "docs/_build/",
    "docs/_themes/",
    "instance/",
    "node_modules/",
    "package-lock.json"
  ],
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "setupFilesAfterEnv": [
      "./tests/setup.js"
    ],
    "testEnvironment": "jsdom",
    "resetMocks": false,
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.0",
    "@popperjs/core": "^2.11.4",
    "bootstrap": "^4.6.1",
    "bootstrap-icons": "^1.8.1",
    "bootstrap4-toggle": "^3.6.1",
    "darkreader": "^4.9.46",
    "highlight.js": "^11.5.0",
    "jquery": "^3.6.0",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.4",
    "concurrently": "^7.0.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.1.0",
    "jest": "^27.4.3",
    "jest-each": "^27.4.2",
    "jest-localstorage-mock": "^2.4.18",
    "lighthouse": "^9.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.0",
    "raw-loader": "^4.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "url-loader": "^4.1.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.1",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "^6.5.1"
  }
}
