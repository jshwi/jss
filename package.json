{
  "name": "jss",
  "version": "1.13.3",
  "engines": {
    "node": "16.x"
  },
  "description": "A Flask web application",
  "private": "true",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "audit": "npm-run-all audit:*",
    "audit:py": "pipenv run audit --deploy --rcfile .github/pyaud.toml",
    "audit:js": "npx prettier -c . && npm run lint:js",
    "build": "npm run webpack:prod",
    "build:dev": "npm run webpack:dev",
    "clean": "pipenv run clean",
    "create:admin": "pipenv run create:admin",
    "db:upgrade": "pipenv run db:upgrade",
    "format": "npm-run-all format:*",
    "format:misc": "prettier -w .",
    "format:py": "npm-run-all format:py:*",
    "format:py:code": "pipenv run format:code",
    "format:py:docs": "pipenv run format:docs",
    "format:py:str": "pipenv run format:str",
    "lighthouse": "lighthouse http://localhost:5000 --view --chrome-flags=--headless --output-path=\"$(mktemp).html\"",
    "lint": "npm-run-all lint:*",
    "lint:js": "npx eslint",
    "lint:py": "pipenv run lint",
    "prebuild:dev": "npm run static:clean",
    "prestart:dev:watch": "npm run static:clean",
    "release": "run-s build db:upgrade create:admin",
    "start": "pipenv run start",
    "start:dev": "pipenv run start:dev",
    "start:dev:watch": "concurrently -n 'WATCH,SERVER' -c 'bgBlue.bold,bgMagenta.bold' 'npm run webpack:watch' 'npm run start:dev'",
    "static:clean": "pipenv run static:clean",
    "static:compile": "pipenv run static:compile",
    "test": "npm-run-all test:*",
    "test:py": "pipenv run test",
    "types": "npm-run-all types:*",
    "types:py": "pipenv run typecheck",
    "webpack:dev": "NODE_ENV=development webpack --progress",
    "webpack:prod": "NODE_ENV=production webpack",
    "webpack:watch": "NODE_ENV=development webpack --watch --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jshwi/jss.git"
  },
  "keywords": [
    "Flask",
    "web",
    "application"
  ],
  "author": "Stephen Whitlock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jshwi/jss/issues"
  },
  "homepage": "https://github.com/jshwi/jss#readme",
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier",
      "plugin:json/recommended"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false
    },
    "rules": {
      "no-param-reassign": "off",
      "no-console": "off",
      "max-len": [
        "error",
        {
          "code": 79,
          "comments": 72,
          "ignoreComments": false,
          "ignoreTrailingComments": false,
          "ignoreUrls": false,
          "ignoreStrings": false,
          "ignoreTemplateLiterals": false,
          "ignoreRegExpLiterals": false
        }
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "__dirname": true,
      "jQuery": true,
      "$": true
    }
  },
  "eslintIgnore": [
    ".mypy_cache/",
    ".pytest_cache/",
    "Pipfile.lock",
    "app/static",
    "dist/",
    "docs/_build/",
    "docs/_themes/",
    "instance/",
    "node_modules/",
    "package-lock.json"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.0",
    "@popperjs/core": "^2.11.2",
    "bootstrap": "^4.6.1",
    "bootstrap-icons": "^1.7.1",
    "bootstrap4-toggle": "^3.6.1",
    "darkreader": "^4.9.40",
    "highlight.js": "^11.3.1",
    "jquery": "^3.6.0",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.8",
    "babel-loader": "^8.1.0",
    "concurrently": "^7.0.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.1.0",
    "lighthouse": "^9.2.0",
    "mini-css-extract-plugin": "^2.4.6",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "raw-loader": "^4.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "url-loader": "^4.1.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "^6.4.1"
  }
}
